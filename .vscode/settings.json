{
    "editor.suggestSelection": "recentlyUsedByPrefix",
    "editor.autoClosingBrackets": "always",
    "editor.autoClosingOvertype": "always",
    "editor.autoClosingQuotes": "always",
    "css.validate":false,
    "scss.validate":false,
    "color-highlight.enable": true,
    "color-highlight.matchWords": true,
    "color-highlight.languages": [
        "*"
      ],
    "editor.parameterHints.cycle": true,
    "editor.renderControlCharacters": true,
    "editor.renderWhitespace": "boundary",
    "editor.tabCompletion": "on",
    "editor.cursorBlinking": "smooth",
    "editor.cursorSmoothCaretAnimation": true,
    "editor.cursorStyle": "line-thin",
    "editor.snippetSuggestions": "top",
    "files.insertFinalNewline": true,
    "files.trimTrailingWhitespace": true,
    "files.trimFinalNewlines": true,
    "files.simpleDialog.enable": true,
    "workbench.editor.highlightModifiedTabs": true,
    "bracketPairColorizer.forceIterationColorCycle": true,
    "bracketPairColorizer.forceUniqueOpeningColor": true,
    "bracketPairColorizer.highlightActiveScope": true,
    "bracketPairColorizer.showBracketsInGutter": true,
    "bracketPairColorizer.showBracketsInRuler": true,
    "emmet.showSuggestionsAsSnippets": true,
    "emmet.triggerExpansionOnTab": true,
    "docthis.authorName": "Ernesto Jara Olveda",
    "git.allowForcePush": true,
    "git.alwaysShowStagedChangesResourceGroup": true,
    "markdown.preview.breaks": true,
    "merge-conflict.autoNavigateNextConflict.enabled": true,
    "npm.enableScriptExplorer": true,
    "npm.enableRunFromFolder": true,
    "prettier.configPath": "./.prettierrc",
    "bracketPairColorizer.colorMode": "Independent",
    "typescript.autoClosingTags": true,
    "typescript.format.insertSpaceAfterCommaDelimiter": true,
    "typescript.format.insertSpaceAfterConstructor": true,
    "typescript.format.insertSpaceAfterFunctionKeywordForAnonymousFunctions": true,
    "typescript.format.insertSpaceAfterKeywordsInControlFlowStatements": true,
    "typescript.format.insertSpaceAfterSemicolonInForStatements": true,
    "typescript.format.placeOpenBraceOnNewLineForControlBlocks": false,
    "javascript.format.insertSpaceAfterConstructor": true,
    "javascript.format.insertSpaceBeforeFunctionParenthesis": true,
    "javascript.suggest.completeJSDocs":true,
    "javascript.suggest.autoImports":true,
    "javascript.preferences.quoteStyle": "double",
    "editor.showUnused": true,
    "editor.codeActionsOnSave": {
        "source.organizeImports": false
    },
    "javascript.suggestionActions.enabled":true,
    "javascript.referencesCodeLens.enabled":true,
    "javascript.updateImportsOnFileMove.enabled":"always",
    "editor.quickSuggestions": {
        "other": true,
        "comments": true,
        "strings": false
    },

    // Controls the delay in ms after which quick suggestions will show up.
    "editor.quickSuggestionsDelay": 10,

    // Controls if suggestions should automatically show up when typing trigger characters
    "editor.suggestOnTriggerCharacters": true,

    // Controls if pressing tab inserts the best suggestion and if tab cycles through other suggestions

    // Controls whether sorting favours words that appear close to the cursor
    "editor.suggest.localityBonus": true,

    // Controls how suggestions are pre-selected when showing the suggest list
    // Enable word based suggestions
    "editor.wordBasedSuggestions": true,
    "editor.cursorSurroundingLinesStyle": "all",
    "workbench.enableExperiments": true,
    "javascript.suggest.includeAutomaticOptionalChainCompletions": true,
    // Enable parameter hints
    "editor.parameterHints.enabled": true,
    "editor.matchBrackets": "always",
    "files.exclude": {
        "**/node_modues": true,
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true
      },
      // Configure glob patterns for excluding files and folders in searches. Inherits all glob patterns from the `files.exclude` setting. Read more about glob patterns [here](https://code.visualstudio.com/docs/editor/codebasics#_advanced-search-options).
      "search.exclude": {
        "**/node_modules": true,
        "**/bower_components": true
      },
    "files.watcherExclude": {
        "**/.git/subtree-cache/**": true,
        "**/node_modules/**": true,
        "**/scripts/**": true
    },
    "jsdoc.author": "Ernesto Jara Olveda",
    "[typescript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "editor.acceptSuggestionOnCommitCharacter": true,
    "editor.autoSurround": "languageDefined",
    "autoimport.doubleQuotes": true,
    "editor.suggest.showColors": true,
    "editor.fontLigatures": true,
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[javascriptreact]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "[javascript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "workbench.editor.revealIfOpen": true,
    "[scss]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "typescript.updateImportsOnFileMove.enabled": "always",
    "[typescriptreact]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "[html]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "editor.suggest.shareSuggestSelections": true,
    "files.autoGuessEncoding": true,
    "workbench.view.alwaysShowHeaderActions": true,
    "workbench.sideBar.location": "right",
    "autoimport.useSemiColon": true,
    "materialTheme.accent": "Yellow",
    "files.associations": {
        "*.ts": "typescript"
    },
    "vsintellicode.features.python.deepLearning": "enabled",
    "vsintellicode.java.completionsEnabled": false,
    "prettier.arrowParens": "always",
    "typescript.tsserver.trace": "messages",
    "npm-intellisense.scanDevDependencies": true,
    "npm-intellisense.importQuotes": "\"",
    "npm-intellisense.packageSubfoldersIntellisense": true,
    "dependencyAnalyticsServer.trace.server": "verbose",
    "window.zoomLevel": 0,
