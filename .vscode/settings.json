    "editor.tokenColorCustomizations": {

        "[Material Gecko]": {

            "keywords": "#ffb600",
            "functions": "#00fff2",
            "types": "#09ff00",
            "strings": "#c0880e",
            "comments": "#00fff2",
            "numbers": "#FF0000",
            "textMateRules": [
                {
                    "scope": [
                        "keyword.operator",
                        "keyword",
                        "keyword.control",
                        "keyword.operator.arithmetic",
                        "keyword.operator.assignment",
                        "keyword.operator.logical",
                        "keyword.operator.new",
                        "keyword.other"
                    ],
                    "settings": {
                        "foreground": "#00ffff",
                        "fontStyle": "italic"
                    }
                },
                {
                    "scope": [
                        "constant",
                        "constant.character",
                        "constant.character.escape",
                        "constant.numeric",
                        "constant.numeric.float",
                        "constant.numeric.hex",
                        "constant.numeric.integer",
                        "constant.numeric.octal",
                        "constant.other",
                        "constant.regexp",
                        "constant.rgb-value",
                        "support.constant"
                    ],
                    "settings": {
                        "foreground": "#00ff0d",
                        "fontStyle": "italic"
                    }
                },
                {
                    "scope": [
                        "emphasis",
                        "entity",
                        "entity.name",
                        "entity.name.*"
                    ],
                    "settings": {
                        "foreground": "#FFb600",
                        "fontStyle": "italic"
                    }
                },
                {
                    "scope": [
                        "markup"
                    ],
                    "settings": {
                        "foreground": "#bbff00",
                        "fontStyle": "italic"
                    }
                },
                {
                    "scope": [
                        "markup.*"
                    ],
                    "settings": {
                        "foreground": "#00ff2a"
                    }
                },
                {
                    "scope": "markup.*.*",
                    "settings": {
                        "foreground": "#00ffea",
                        "fontStyle": "italic"
                    }
                },
                {
                    "scope": [
                        "variable",
                    ],
                    "settings": {
                        "foreground": "#9900ff",
                        "fontStyle": "italic"
                    }
                },
                {
                    "scope": "variable.name",
                    "settings": {
                        "foreground": "#ff00ff",
                        "fontStyle": "italic"
                    }
                },
                {
                    "scope": [
                        "variable.other.readwrite"
                    ],
                    "settings": {
                        "foreground": "#ff0055",
                        "fontStyle": "italic"
                    }
                }
            ]

        }
    },
